<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.liangyunfeng.mvvm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".base.BaseActivity"></activity>
        <activity android:name=".recyclerview.RecyclerViewActivity"></activity>
        <activity android:name=".bindingadapter.BindingAdapterActivity"></activity>
        <activity android:name=".map.ArrayMapActivity"></activity>
        <activity android:name=".symbol.SymbolActivity"></activity>
        <activity android:name=".bindingmethod.BindingMethodActivity"></activity>
        <activity android:name=".lambda.OnClickActivity"></activity>
        <activity android:name=".two_way_data_binding.TwoWayDataBindingActivity"></activity>
        <activity android:name=".two_way_data_binding2.ProgressActivity"></activity>
    </application>

</manifest>